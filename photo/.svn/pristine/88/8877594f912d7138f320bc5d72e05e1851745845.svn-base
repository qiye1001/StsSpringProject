package Bean;

public class Emp {
	String name;
	int age;
	int saraly;
	
	public Emp() {
		super();
	}
	public Emp(String name, int age, int saraly) {
		super();
		this.name = name;
		this.age = age;
		this.saraly = saraly;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAgs() {
		return age;
	}
	public void setAgs(int age) {
		this.age = age;
	}
	public int getSaraly() {
		return saraly;
	}
	public void setSaraly(int saraly) {
		this.saraly = saraly;
	}
	
	public String getInfo() {
		return "Emp [name=" + name + ", age=" + age + ", saraly=" + saraly + "]";
	}
	
	
}
